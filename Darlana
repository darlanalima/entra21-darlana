    }

20) let numero = Number(prompt("digite um numero de 1 a 12"))
    switch(numero){
    case 1:
    console.log("janeiro")
    break
    case 2:
    console.log("fevereiro")
    break
    case 3:
    console.log("março")
    break
    case 4:
    console.log("abril")
    break
    case 5:
    console.log("maio")
    break
    case 6:
    console.log("junho")
    break
    case 7:
    console.log("julho")
    break
    case 8:
    console.log("agosto")
    break
    case 9:
    console.log("setembro")
    break
    case 10:
    console.log("outubro")
    break
    case 11:
    console.log("novembro")
    break
    case 12:
    console.log("dezembro")
    break
    default:
    console.log("numero invalido")
    break
} 

21) let turno = prompt("em qual turno voce estuda?M-matutino ou V-Vespertino ou N- Noturno")
    switch (turno){
    case "M": 
    console.log("bom dia")
    break
    case "V":
    console.log("boa tarde")
    break
    case "N":
    console.log("boa noite")
    break
    default:
    console.log("valor invalido")
    break
}

9 let vetor = []  
        for(let i = 0 ; i < 8; i++){
            vetor[i]= +prompt("digite um numero")
        }

        let x, y
        while (true) {
            x = +prompt("Digite o valor de X")
            if (x > 7 || x < 0) {
                alert("Você digitou um valor inválido")
                continue
            }   
            break     
        }

        while (true) {
            y = +prompt("Digite o valor de Y")
            if (y > 7 || y < 0) {
                alert("Você digitou um valor inválido")
                continue
            }
            break
        }

        let soma = vetor[x] + vetor[y]
        console.log(`A soma do vetor na posição ${x} e ${y} = ${soma}`)
