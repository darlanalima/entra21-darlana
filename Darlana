     }

7)   Boolean(A > 10 && B <= 20) // FALSE
     Boolean(A > 10 || B <= 20) // TRUE
     Boolean((A === 10 || B > 20) && B === 20) // TRUE
     console.log(null || 2 || undefined) // 2
     Boolean(0 && 1) // FALSE

8)    let valorA = Number(prompt("digite um numero 1"))
      let valorB = Number(prompt("digite um numero 2"))
      let valorC = Number(prompt("digite um numero 3"))
      if (valorA > valorB && valorA > valorC){
      console.log(`o maior numero é: ${valorA}`)
     } else if (valorB > valorA && valorB > valorC){
      console.log(`o maior numero é: ${valorB}`)
     } else {
      console.log(`o maior numero é: ${valorC}`)
     }

9)   let resultado = ""
     let a = Number(prompt("primeiro numero"))
     let b = Number(prompt("segundo numero"))
     a + b < 4 ? resultado = "Abaixo" : resultado = "Acima"
     console.log (`resultado: ${resultado}`)

10) let lado1= Number(prompt("digite o tamanho dos lados"))
    let lado2 = Number(prompt("digite o tamanho do segundo lado"))
    let lado3 = Number(prompt("digite o tamanho do terceiro lado"))
    if (lado1 === lado2 && lado1 === lado3){
        console.log("esse triangulo é equilatero")
    } else if (lado1 === lado2 || lado1 === lado3 || lado2 === lado3){
        console.log ("esse triangulo é isosceles")
    } else { 
        console.log ("esse triangulo é escaleno")
    } 

11) let numero= Number(prompt("digite um numero"))
    if (numero >= 0){
        console.log(`a raiz quadrada do numero é: ${Math.sqrt(numero).toFixed(2)}`)
    } else {
        console.log(`o numero é invalido`)
    }

